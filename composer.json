{
    "name": "senet-eindhoven/cartograph",
    "description": "Map objects to objects",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "mapping",
        "object mapping"
    ],
    "homepage": "https://www.senet.nl/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4||^8.0",
        "psr/container": "^1.0||^2.0"
    },
    "suggest": {
        "doctrine/orm": "Allows usage of the EntityMappingRepository, which handles proxy classes."
    },
    "autoload": {
        "psr-4": {
            "Senet\\Cartograph\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Senet\\Cartograph\\": "test/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    }
}
